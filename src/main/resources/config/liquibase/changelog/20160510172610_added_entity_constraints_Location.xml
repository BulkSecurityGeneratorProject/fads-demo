<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <!--
        Added the constraints for entity Location.
    -->
    <changeSet id="20160510172610-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="organizationlocation_id"
                                 baseTableName="location"
                                 constraintName="fk_location_organizationlocation_id"
                                 referencedColumnNames="id"
                                 referencedTableName="organizationlocation"/>

        <addForeignKeyConstraint baseColumnNames="personlocation_id"
                                 baseTableName="location"
                                 constraintName="fk_location_personlocation_id"
                                 referencedColumnNames="id"
                                 referencedTableName="personlocation"/>

    </changeSet>
</databaseChangeLog>
